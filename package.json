{
  "name": "response-formatter",
  "version": "0.0.1",
  "description": "Format response to",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/_mocha --recursive",
    "coveralls": "npm install istanbul && node --version && node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha --report lcovonly -- test --recursive -R spec && cat coverage/lcov.info | node_modules/coveralls/bin/coveralls.js && rm -rf coverage",
    "coverage": "istanbul cover node_modules/.bin/_mocha -- --recursive",
    "postcoverage": "istanbul report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NumminorihSF/response-formatter.git"
  },
  "keywords": [
    "express",
    "response",
    "xml",
    "text/xml",
    "json",
    "application/json",
    "text/plain",
    "text/html"
  ],
  "author": "Konstantine Petryaev <numminorihsf@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NumminorihSF/response-formatter/issues"
  },
  "homepage": "https://github.com/NumminorihSF/response-formatter#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.6",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.0.0"
  },
  "dependencies": {
    "xml2js": "^0.4.16"
  }
}
